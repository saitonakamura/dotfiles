{
  "editor.lineNumbers": "relative",
  "editor.suggestSelection": "first",
  "editor.fontFamily": "'Cascadia Code PL', 'FiraCode Retina', 'Fira Code', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.find.addExtraSpaceOnTop": false,
  "workbench.iconTheme": "vscode-icons",
  "workbench.preferredDarkColorTheme": "Atom One Dark",
  "workbench.preferredLightColorTheme": "Atom One Light",
  "workbench.settings.editor": "json",
  "window.autoDetectColorScheme": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "git.autofetch": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "terminal.integrated.fontFamily": "Cascadia Code PL",
  "terminal.external.windowsExec": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
  "vim.leader": " ",
  "vim.useSystemClipboard": true,
  "vim.replaceWithRegister": true,
  "vim.surround": true,
  "vim.highlightedyank.enable": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", ";"],
      "after": [":"]
    },
    {
      "before": ["<leader>", "]"],
      "commands": ["workbench.action.navigateForward"]
    },
    {
      "before": ["<C-i>"],
      "commands": ["workbench.action.navigateForward"]
    },
    {
      "before": ["<leader>", "["],
      "commands": ["workbench.action.navigateBack"]
    },
    {
      "before": ["<C-o>"],
      "commands": ["workbench.action.navigateBack"]
    },
    {
      "before": ["<leader>", "b", "["],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["<leader>", "b", "]"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<leader>", "b", "d"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "b", "D"],
      "commands": ["workbench.action.closeAllEditors"]
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["<leader>", "n", "t"],
      "commands": ["workbench.action.terminal.focus"]
    },
    {
      "before": ["<leader>", "n", "p"],
      "commands": ["revealInExplorer"]
    },
    {
      "before": ["<leader>", "n", "b"],
      "commands": ["workbench.files.action.focusOpenEditorsView"]
    },
    {
      "before": ["<leader>", "n", "f"],
      "commands": ["workbench.view.search"]
    },
    {
      "before": ["<leader>", "n", "r"],
      "commands": ["workbench.action.replaceinfiles"]
    },
    {
      "before": ["<leader>", "t", "s"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "t", "p"],
      "commands": ["workbench.action.togglePanel"]
    },
    {
      "before": ["<C-W>", "Q"],
      "commands": ["workbench.action.closeEditorsInGroup"]
    },
    {
      "before": ["<leader>", "g", "d"],
      "commands": ["editor.action.revealDefinition"]
    },
    {
      "before": ["<leader>", "g", "t"],
      "commands": ["editor.action.goToTypeDefinition"]
    },
    {
      "before": ["<leader>", "g", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["<leader>", "g", "r"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["<leader>", "g", "s"],
      "commands": ["workbench.action.showAllSymbols"]
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "k"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["<leader>", "p", "d"],
      "commands": ["editor.action.peekDefinition"]
    },
    {
      "before": ["<leader>", "p", "r"],
      "commands": ["editor.action.findReferences"]
    },
    {
      "before": ["<leader>", "p", "s"],
      "commands": ["workbench.action.gotoSymbol"]
    },
    {
      "before": ["<leader>", "a", "r"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "a", "m"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["<leader>", "a", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "r", "r"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "W"],
      "commands": ["workbench.action.files.saveAll"]
    },
    {
      "before": ["<leader>", "d", "b"],
      "commands": ["editor.debug.action.toggleBreakpoint"],
      "when": "editorTextFocus"
    },
    {
      "before": ["<leader>", "v", "c"],
      "commands": ["git.commit"]
    },
    {
      "before": ["<leader>", "v", "s"],
      "commands": ["git.checkout"]
    },
    {
      "before": ["<leader>", "v", "S"],
      "commands": ["git.branch"]
    },
    {
      "before": ["<leader>", "v", "v"],
      "commands": ["workbench.view.scm"]
    },
    {
      "before": ["<leader>", "v", "f"],
      "commands": ["git.fetch"]
    },
    {
      "before": ["<leader>", "v", "p"],
      "commands": ["git.push"]
    },
    {
      "before": ["<leader>", "v", "l"],
      "commands": ["git.pull"]
    },
    {
      "before": ["<leader>", "v", "b"],
      "commands": ["gitlens.toggleFileBlame"]
    },
    {
      "before": ["<leader>", "v", "k"],
      "commands": ["gitlens.showQuickCommitDetails"]
    },
    {
      "before": ["<leader>", "/"],
      "commands": [":nohlsearch"]
    },
    {
      "before": ["g", "/"],
      "commands": ["/"]
    },
    {
      "before": ["<leader>", "a", "c", "p"],
      "commands": ["copyRelativeFilePath"]
    },
    {
      "before": ["<leader>", "o"],
      "after": ["o", "<Esc>"]
    },
    {
      "before": ["<leader>", "O"],
      "after": ["O", "<Esc>"]
    },
    {
      "before": ["<leader>", "m", "m"],
      "commands": ["bookmarks.toggle"]
    },
    {
      "before": ["<leader>", "m", "["],
      "commands": ["bookmarks.jumpToNext"]
    },
    {
      "before": ["<leader>", "m", "]"],
      "commands": ["bookmarks.jumpToPrevious"]
    },
    {
      "before": ["<leader>", "n", "m"],
      "commands": ["bookmarksExplorer.focus"]
    },
    {
      "before": ["<leader>", "p", "m"],
      "commands": ["bookmarks.list"]
    },
    // https://github.com/VSCodeVim/Vim/issues/8157
    // Undo & Redo
    { "before": ["u"], "commands": ["undo"] },
    { "before": ["c-r"], "commands": ["redo"] }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", ";"],
      "after": [":"]
    },
    {
      "before": ["<leader>", "]"],
      "commands": ["workbench.action.navigateForward"]
    },
    {
      "before": ["<C-i>"],
      "commands": ["workbench.action.navigateForward"]
    },
    {
      "before": ["<leader>", "["],
      "commands": ["workbench.action.navigateBack"]
    },
    {
      "before": ["<C-o>"],
      "commands": ["workbench.action.navigateBack"]
    },
    {
      "before": ["/"],
      "commands": ["actions.find"]
    },
    {
      "before": ["<leader>", "n", "g"],
      "commands": ["workbench.view.search"]
    },
    {
      "before": ["<leader>", "n", "r"],
      "commands": ["workbench.action.replaceInFiles"]
    },
    {
      "before": ["<leader>", "a", "f"],
      "commands": ["editor.action.formatSelection"]
    },
    {
      "before": ["<leader>", "a", "m"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["workbench.action.showCommands"]
    }
  ],
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "jest.autoRun": { "watch": false, "onSave": ["test-file"] },
  "zenMode.centerLayout": false,
  "zenMode.fullScreen": false,
  "editor.minimap.enabled": false,
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "emmet.excludeLanguages": [
    "typescriptreact",
    "typescript",
    "javascriptreact",
    "javascript",
    "markdown"
  ],
  "workbench.editor.wrapTabs": true,
  "editor.accessibilitySupport": "off",
  "workbench.colorTheme": "Atom One Light",
  "vsicons.dontShowNewVersionMessage": true,
  "window.zoomLevel": 1,
  "hexeditor.columnWidth": 26,
  "hexeditor.showDecodedText": true,
  "hexeditor.defaultEndianness": "little",
  "hexeditor.inspectorType": "aside",
  "redhat.telemetry.enabled": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "terminal.integrated.tabs.enabled": false,
  "gitlens.mode.active": "zen"
}
